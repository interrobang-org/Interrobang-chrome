{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": [
    "activeTab", 
    "declarativeContent", 
    "storage",
    "https://httpbin.org/*",
    "http://127.0.0.1:5000/*"
  ],
  "options_page": "assets/html/options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "assets/html/popup.html",
    "default_icon": {
      "16": "assets/images/favicon-16.png",
      "32": "assets/images/favicon-32.png",
      "48": "assets/images/favicon-32.png",
      "128": "assets/images/favicon-192.png"
    }
  },
  "icons": {
    "16": "assets/images/favicon-16.png",
    "32": "assets/images/favicon-32.png",
    "48": "assets/images/favicon-32.png",
    "128": "assets/images/favicon-192.png"
  },
  "manifest_version": 2
}
